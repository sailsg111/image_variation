{
    "manifest_version": 2,
    "name": "ImageReplacer",
    "version": "1",
    "web_accessible_resources": [
        "images/*.jpg"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}